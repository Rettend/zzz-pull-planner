{
  "name": "zzz-pull-planner",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.3.2",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vinxi dev --port 3101",
    "build": "bun --bun vinxi build",
    "preview": "wrangler dev .output/server/index.mjs --assets .output/public",
    "check": "tsgo --noEmit --skipLibCheck -p tsconfig.json",
    "lint": "eslint . --fix",
    "l:push": "drizzle-kit push --config drizzle.config.local.ts",
    "l:studio": "drizzle-kit studio --config drizzle.config.local.ts",
    "l:seed": "bun run scripts/seed.ts",
    "r:generate": "drizzle-kit generate",
    "r:migrate": "wrangler d1 migrations apply zzz-pull-planner --remote",
    "r:studio": "drizzle-kit studio --config drizzle.config.ts",
    "worker:dev": "wrangler dev src/worker/index.ts",
    "deploy": "bun run build && wrangler deploy",
    "deploy:scraper": "wrangler deploy --config wrangler.worker.toml",
    "update": "taze latest -wr"
  },
  "dependencies": {
    "@solid-primitives/storage": "^4.3.3",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.4",
    "@solidjs/start": "^1.2.0",
    "@typescript/native-preview": "^7.0.0-dev.20251120.1",
    "cheerio": "^1.1.2",
    "drizzle-orm": "^0.44.7",
    "html-to-image": "^1.11.13",
    "solid-js": "^1.9.10",
    "uuid": "^13.0.0",
    "vinxi": "^0.5.8",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@cloudflare/workers-types": "^4.20251120.0",
    "@iconify-json/ph": "^1.2.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.1",
    "@unocss/eslint-plugin": "^66.5.7",
    "@unocss/preset-icons": "^66.5.7",
    "better-sqlite3": "^12.4.1",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-plugin-solid": "^0.14.5",
    "happy-dom": "^20.0.10",
    "taze": "^19.9.0",
    "unocss": "^66.5.7",
    "vitest": "^4.0.12",
    "wrangler": "^4.49.1"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "better-sqlite3",
    "esbuild",
    "sharp",
    "workerd"
  ]
}
