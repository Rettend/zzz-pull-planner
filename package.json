{
  "name": "zzz-pull-planner",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.2.21",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vinxi dev --port 3101",
    "build": "bun --bun vinxi build",
    "preview": "vinxi start",
    "check": "tsgo --noEmit --skipLibCheck -p tsconfig.json",
    "lint": "eslint . --fix",
    "l:push": "drizzle-kit push --config drizzle.config.local.ts",
    "l:studio": "drizzle-kit studio --config drizzle.config.local.ts",
    "l:seed": "bun run scripts/seed.ts",
    "r:generate": "drizzle-kit generate",
    "r:migrate": "wrangler d1 migrations apply zzz-pull-planner --remote",
    "r:studio": "drizzle-kit studio --config drizzle.config.ts",
    "worker:dev": "wrangler dev src/worker/index.ts",
    "deploy": "bun run build && wrangler deploy",
    "deploy:scraper": "wrangler deploy --config wrangler.worker.toml"
  },
  "dependencies": {
    "@solid-primitives/storage": "^4.3.3",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.7",
    "@typescript/native-preview": "^7.0.0-dev.20251016.1",
    "cheerio": "^1.1.2",
    "drizzle-orm": "^0.44.7",
    "solid-js": "^1.9.9",
    "uuid": "^12.0.0",
    "vinxi": "^0.5.8",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@cloudflare/workers-types": "^4.20251119.0",
    "@iconify-json/ph": "^1.2.2",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.3.1",
    "@unocss/eslint-plugin": "^66.5.3",
    "@unocss/preset-icons": "^66.5.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.37.0",
    "eslint-plugin-solid": "^0.14.5",
    "happy-dom": "^20.0.10",
    "unocss": "^66.5.0",
    "vitest": "^4.0.10",
    "wrangler": "^4.49.0"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "esbuild",
    "sharp",
    "workerd"
  ]
}
